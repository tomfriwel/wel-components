/* components/wel-cell.wxss */
.wcell-container {
    position: relative;
    width: 750rpx;
    height: 88rpx;
    background: white;
}

.wcell-top-line {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 2rpx;
    background-color: #EAEAEC;
}

.wcell-bottom-line {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 2rpx;
    background-color: #EAEAEC;
}